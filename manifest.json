{
  "manifest_version": 2,

  "name": "Fast-Translate",
  "description": "This extension easy you life, translate real time very fast!",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon_brasil.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "http://api.mymemory.translated.net/get",
    "tabs",
    "http://*/*"
  ]

}
